{
  "name": "angular-typescript",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean-step:dist": "node lib/rimraf-standalone.js dist",
    "clean-step:node_modules": "node lib/rimraf-standalone.js node_modules",
    "build-step:dev": "npm run serve",
    "build-step:prd": "npm run build",
    "build-step:lint": "tslint --project tsconfig.json ./src/**/*.ts",
    "pipeline:clean": "npm run pipeline:clean:dev",
    "pipeline:clean:dev": "npm run clean-step:dist",
    "pipeline:clean:all": "npm run clean-step:dist && npm run clean-step:node_modules",
    "pipeline:build": "npm run pipeline:build:dev",
    "pipeline:build:dev": "npm run pipeline:clean:dev && npm run build-step:lint && npm run build-step:dev",
    "pipeline:build:prd": "npm run pipeline:clean:all && npm install && npm run build-step:lint && npm run e2e && npm run build-step:prd"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "bootstrap": "^4.0.0",
    "core-js": "^2.4.1",
    "ng2-logger": "^2.0.32",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.6",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.6.0"
  }
}
